{
  "name": "rdf-object",
  "version": "1.0.0",
  "description": "Loads RDF streams in memory as JSON objects",
  "keywords": [
    "rdf",
    "object",
    "stream",
    "resource",
    "rdfjs",
    "linked data"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/rdf-object.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/rdf-object.js/issues"
  },
  "homepage": "https://github.com/rubensworks/rdf-object.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@rdfjs/data-model": "^1.1.0",
    "@types/jest": "^23.3.0",
    "@types/n3": "^1.0.0",
    "@types/rdf-js": "^1.0.1",
    "arrayify-stream": "^1.0.0",
    "coveralls": "^3.0.0",
    "jest": "^23.4.1",
    "jest-rdf": "^1.1.0",
    "n3": "^1.0.0-beta.2",
    "pre-commit": "^1.2.2",
    "rdf-quad": "^1.2.2",
    "streamify-string": "^1.0.1",
    "ts-jest": "^23.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-rdf",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.1.0",
    "rdf-string": "^1.2.0"
  }
}
